<ModalDialog @title="Sign up requirements have not been met" @isInline={{@isInline}} as |Modal|>
  <Modal.body>
    <ScheduleRequirements @requirements={{this.data.requirements}} @person={{this.data.person}} />
    <p>
      <b class="text-danger">You have the privileges to force the sign up and your actions will be logged if you do.</b>
    </p>
  </Modal.body>
  <Modal.footer>
    <UiCancelButton @onClick={{@onClose}} />
    <UiButton @type="warning" @onClick={{@onConfirm}}>Force Sign Up</UiButton>
  </Modal.footer>
</ModalDialog>
