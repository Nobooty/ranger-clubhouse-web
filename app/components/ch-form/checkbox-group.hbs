{{#each checkboxColumns as |column|}}
  <div class="{{gridColumn}}">
    {{#each column as |cb|}}
      <div class="form-check">
        <input
          type="checkbox"
          id={{cb._domId}}
          class="form-check-input"
          checked={{cb.isChecked}}
          onclick={{action "onClick" cb}}
        >
        <label for="{{cb._domId}}" class="form-check-label">{{cb.label}}</label>
       </div>
    {{/each}}
  </div>
{{/each}}
